#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss@3.0.13_f98f8cb1d4c2734d9765edb689db6e16/node_modules/tailwindcss/lib/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss@3.0.13_f98f8cb1d4c2734d9765edb689db6e16/node_modules/tailwindcss/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss@3.0.13_f98f8cb1d4c2734d9765edb689db6e16/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/tailwindcss/lib/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/tailwindcss/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss@3.0.13_f98f8cb1d4c2734d9765edb689db6e16/node_modules/tailwindcss/lib/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss@3.0.13_f98f8cb1d4c2734d9765edb689db6e16/node_modules/tailwindcss/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss@3.0.13_f98f8cb1d4c2734d9765edb689db6e16/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/tailwindcss/lib/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/tailwindcss/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
