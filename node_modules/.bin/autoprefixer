#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.5/node_modules/autoprefixer/bin/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.5/node_modules/autoprefixer/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.5/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/autoprefixer/bin/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/autoprefixer/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.5/node_modules/autoprefixer/bin/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.5/node_modules/autoprefixer/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.5/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/autoprefixer/bin/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/autoprefixer/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
else
  exec node  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
fi
