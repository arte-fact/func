#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/resolve@1.21.0/node_modules/resolve/bin/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/resolve@1.21.0/node_modules/resolve/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/resolve@1.21.0/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tslint@6.1.3_typescript@4.5.4/node_modules/tslint/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tslint@6.1.3_typescript@4.5.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/resolve@1.21.0/node_modules/resolve/bin/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/resolve@1.21.0/node_modules/resolve/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/resolve@1.21.0/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tslint@6.1.3_typescript@4.5.4/node_modules/tslint/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tslint@6.1.3_typescript@4.5.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../resolve@1.21.0/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../resolve@1.21.0/node_modules/resolve/bin/resolve" "$@"
fi
