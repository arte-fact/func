#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/bin/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/typescript@4.5.4/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/@typescript-eslint+type-utils@5.9.1_eslint@6.8.0+typescript@4.5.4/node_modules/@typescript-eslint/type-utils/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/@typescript-eslint+type-utils@5.9.1_eslint@6.8.0+typescript@4.5.4/node_modules/@typescript-eslint/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/@typescript-eslint+type-utils@5.9.1_eslint@6.8.0+typescript@4.5.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/bin/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/typescript@4.5.4/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/@typescript-eslint+type-utils@5.9.1_eslint@6.8.0+typescript@4.5.4/node_modules/@typescript-eslint/type-utils/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/@typescript-eslint+type-utils@5.9.1_eslint@6.8.0+typescript@4.5.4/node_modules/@typescript-eslint/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/@typescript-eslint+type-utils@5.9.1_eslint@6.8.0+typescript@4.5.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typescript@4.5.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../../typescript@4.5.4/node_modules/typescript/bin/tsserver" "$@"
fi
