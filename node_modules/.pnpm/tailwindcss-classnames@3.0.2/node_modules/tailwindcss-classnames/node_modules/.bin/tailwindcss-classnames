#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss-classnames@3.0.2/node_modules/tailwindcss-classnames/lib/cli/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss-classnames@3.0.2/node_modules/tailwindcss-classnames/lib/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss-classnames@3.0.2/node_modules/tailwindcss-classnames/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss-classnames@3.0.2/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss-classnames@3.0.2/node_modules/tailwindcss-classnames/lib/cli/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss-classnames@3.0.2/node_modules/tailwindcss-classnames/lib/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss-classnames@3.0.2/node_modules/tailwindcss-classnames/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/tailwindcss-classnames@3.0.2/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli/index.js" "$@"
else
  exec node  "$basedir/../../lib/cli/index.js" "$@"
fi
