#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/acorn@7.4.1/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/espree@6.2.1/node_modules/espree/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/espree@6.2.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/acorn@7.4.1/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/espree@6.2.1/node_modules/espree/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/espree@6.2.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@7.4.1/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@7.4.1/node_modules/acorn/bin/acorn" "$@"
fi
