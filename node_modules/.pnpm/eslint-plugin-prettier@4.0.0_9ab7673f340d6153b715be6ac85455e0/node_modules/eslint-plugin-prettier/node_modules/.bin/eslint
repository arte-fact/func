#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/eslint@6.8.0/node_modules/eslint/bin/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/eslint@6.8.0/node_modules/eslint/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/eslint@6.8.0/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/eslint-plugin-prettier@4.0.0_9ab7673f340d6153b715be6ac85455e0/node_modules/eslint-plugin-prettier/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/eslint-plugin-prettier@4.0.0_9ab7673f340d6153b715be6ac85455e0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/eslint@6.8.0/node_modules/eslint/bin/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/eslint@6.8.0/node_modules/eslint/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/eslint@6.8.0/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules:/home/benoit/IdeaProjects/node_modules:/home/benoit/node_modules:/home/node_modules:/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/eslint-plugin-prettier@4.0.0_9ab7673f340d6153b715be6ac85455e0/node_modules/eslint-plugin-prettier/node_modules:/home/benoit/IdeaProjects/func-ts/node_modules/.pnpm/eslint-plugin-prettier@4.0.0_9ab7673f340d6153b715be6ac85455e0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@6.8.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@6.8.0/node_modules/eslint/bin/eslint.js" "$@"
fi
